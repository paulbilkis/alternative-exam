/*
Рисует бинарное дерево из скобочного представления
*/
function draw_bin_tree (br){
    var levels = []; // хранение статуса занятости уровней
    for (var i = 0; i < br.length; i++){
	if (br[i] == 1){
	    // рисуем ветвь влево, нумеруем i+1
	}else{
	    for (var j=0; levels[j]!=null; j++); // j по итогу - номер ближайшего свободного уровня
	    // рисуем на этом уровне ветвь вправо
	    levels[j] = 1;
	}
    }
}
